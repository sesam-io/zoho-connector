{
    "_id": "{{@ system @}}",
    "headers": {
        "Authorization": "Zoho-oauthtoken $SECRET(jwt)",
        "Content-Type": "application/json"
    },
    "operations": {
        "get": {
            "method": "GET",
            "url": "{{ properties.module }}?fields={{ properties.fields }}"
        }
    },
    "type": "system:rest",
    "url_pattern": "https://www.zohoapis.eu/crm/v3/%s",
    "verify_ssl": true
}